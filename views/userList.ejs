<!DOCTYPE html>
<html>
<head>
    <title>Listă de Utilizatori</title>
</head>
<body>
    <h1>Listă de Utilizatori</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Nume</th>
            <th>Address</th>
            <th>Phone Number</th>
            <th>Email Address</th>
        </tr>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.name %></td>
                <td><%= user.address %></td>
                <td><%= user.phone_number %></td>
                <td><%= user.email_address %></td>
            </tr>
        <% }); %>
    </table>
</body>
</
